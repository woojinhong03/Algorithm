      "source": [
        "arr = [[0]*100 for _ in range(100)]\n",
        "\n",
        "for _ in range(int(input())):\n",
        "    m, n = map(int, input().split())\n",
        "    for i in range(m, m+10):\n",
        "        for j in range(n, n+10):\n",
        "            arr[i][j] = 1\n",
        "\n",
        "ans = 0\n",
        "for i in arr:\n",
        "    ans += i.count(1)\n",
        "\n",
        "print(ans)"
      ]
    }
