        "n = input()\n",
        "\n",
        "comp = len(n) - 1\n",
        "\n",
        "answer = 0\n",
        "\n",
        "for i in range(comp):\n",
        "    answer += 9 * (10 ** i) * (i + 1)\n",
        "    i += 1\n",
        "answer += ((int(n) - (10 ** comp)) + 1) * (comp + 1)\n",
        "\n",
        "print(answer)"
      ]
