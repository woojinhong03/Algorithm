        "i = 0\n",
        "while True:\n",
        "    i+=1\n",
        "    l, p, v = map(int, input().split())\n",
        "\n",
        "    if l == 0 and p == 0 and v == 0:\n",
        "        break\n",
        "\n",
        "    a = v//p\n",
        "    b = v%p\n",
        "\n",
        "    if l<b:\n",
        "        b = l\n",
        "        \n",
        "    print(\"Case %d: %d\" %(i, a*l+b))"
