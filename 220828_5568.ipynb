      "source": [
        "from itertools import permutations\n",
        "\n",
        "n, k = int(input()), int(input())\n",
        "cards = [input().rstrip() for _ in range(n)]\n",
        "res = set()\n",
        "for per in permutations(cards, k):\n",
        "    res.add(''.join(per))\n",
        "    \n",
        "print(len(res))"
      ]
    }
