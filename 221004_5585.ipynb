        "m = [500,100,50,10,5,1]\n",
        "p = 1000 - int(input())\n",
        "cnt = 0\n",
        "for i in m:\n",
        "    if p >= i:\n",
        "        while p >= i:\n",
        "            p -= i\n",
        "            cnt += 1\n",
        "print(cnt)\n"
