        "n, p = map(int, input().split())\n",
        "\n",
        "l = []\n",
        "a = n\n",
        "\n",
        "while True:\n",
        "    a = (a * n) % p\n",
        "    if a in l:\n",
        "        print(len(l) - l.index(a))\n",
        "        break\n",
        "    l.append(a)"
